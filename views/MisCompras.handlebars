<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Celuventas</h1>
    <p class="lead">Mis ventas</p>
  </div>
</div>
{{#each resultado}}
<div class="container">
	<table id="cart" class="table table-hover table-condensed">
    				<thead>
						<tr>
							<th style="width:50%">Producto</th>
							<th style="width:10%">Precio</th>
							<th style="width:8%">Quantity</th>
							<th style="width:22%" class="text-center">Fecha</th>
							<th style="width:10%"></th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td data-th="Product">
								<div class="row">
									<div class="col-sm-2 hidden-xs"><img src={{{this.Imagen}}} alt="..." class="img-responsive"/></div>
									<div class="col-sm-10">
										<h4 class="nomargin">{{{this.Marca}}} {{{this.Modelo}}} </h4>
										<p>Capacidad: {{{this.Capacidad}}} Color: {{{this.Color}}} </p>
									</div>
								</div>
							</td>
							<td data-th="Price">${{{this.Precio}}} </td>
							<td data-th="Quantity" class="text-center">1</td>
								{{!-- <input type="text" class="form-control text-center" value="1"> --}}
					
							
						
						</tr>
					</tbody>
					
				</table>
</div>

{{/each}}
<center>
<tfoot>
					
							<td><a href="/volver" class="btn btn-warning"><i class="fa fa-angle-left"></i> Volver al Menu</a></td>
							<td colspan="2" class="hidden-xs"></td>
							
					</tfoot>

</center>
